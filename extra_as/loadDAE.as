package {	import PaperBase;	import org.papervision3d.objects.primitives.Cone;	import org.papervision3d.materials.*;	import org.papervision3d.materials.BitmapMaterial;	import org.papervision3d.objects.DisplayObject3D;	import org.papervision3d.objects.parsers.Collada;	import org.papervision3d.objects.parsers.DAE;	import org.papervision3d.materials.utils.*;	import org.papervision3d.objects.primitives.Plane;		//import BitmapContainer;	import flash.display.*;	import flash.filters.*;	public class loadDAE extends PaperBase {		var model_path:String = "./assets/3D Models/";		var lung_path:String = model_path+"lung/";		var brain_path:String = model_path+"bio_hand/";		var lung:DAE = new DAE(true, null,true);		protected var sceneWidth:Number;		protected var sceneHeight:Number;		public function loadDAE() {			sceneWidth=stage.stageWidth;			sceneHeight=stage.stageHeight;			init(sceneWidth,sceneHeight);		}		override protected function init3d():void {						//lung.load(lung_path+"lung_test.dae");			lung.load(brain_path+"bio_hand.dae");			lung.moveDown(1000);			lung.scale=100;			//lung.pitch(-10);			default_scene.addChild(lung);		}		override protected function processFrame():void {						lung.yaw(-0.01);			//lung.pitch( -5);					}	}}